<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="TrimFuel" type="TrimFuel.Business.Configuration.TrimFuelSection, TrimFuel.Business"/>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <TrimFuel AppAuthMode="Static">
    <StaticAppAuth ApplicationId="BlokedEmailDomainListImport"/>
    <Applications>
      <add ApplicationId="BlokedEmailDomainListImport">
        <ConnectionStrings>
          <add name="TrimFuel" connectionString="Datasource=localhost;Database=trimfuellocal;uid=root;pwd=1;"/>
        </ConnectionStrings>
      </add>
    </Applications>
  </TrimFuel>
  <appSettings>
    <add key="limit" value="1"/>
  </appSettings>
  <log4net>
    <root>
      <appender-ref ref="GeneralLog"/>
    </root>
    <appender name="GeneralLog" type="log4net.Appender.RollingFileAppender">
      <file value="errors.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="-1"/>
      <maximumFileSize value="50GB"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%newline%message"/>
      </layout>
    </appender>
  </log4net>
  <startup>
    <supportedRuntime version="v2.0.50727"/>
  </startup>
</configuration>
